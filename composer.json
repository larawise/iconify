{
    "name"                  : "larawise/iconify",
    "type"                  : "package",
    "description"           : "A fluent, cache-aware, testable icon system for Laravel. Resolve icons with precision, render them with clarity, and stream thousands with confidence — all through a single semantic interface.",
    "keywords"              : ["svg","semantic","laravel","manager","rendering","icons","lazy","blade","loading","icon","reuse","fluent","iconify","referencing","srylius"],

    "license"               : "MIT",
    "homepage"              : "https://larawise.com/",

    "authors"               : [{
        "name"            : "Selçuk Çukur",
        "email"           : "hk@selcukcukur.com.tr",
        "homepage"        : "https://selcukcukur.com.tr"
    }],

    "support"               : {
        "source"          : "https://github.com/larawise/iconify",
        "issues"          : "https://github.com/larawise/iconify/issues",
        "docs"            : "https://docs.larawise.com"
    },

    "require"               : {
        "php"                               : "^8.2|^8.3|^8.4",
        "larawise/larawise"                 : "dev-main",
        "larawise/packagify"                : "dev-main"
    },
    "require-dev"           : {
        "symfony/var-exporter"              : "^7.3"
    },

    "autoload"              : {
        "psr-4"     : {
            "Larawise\\Iconify\\"           : "./src"
        }
    },
    "autoload-dev"          : {
        "psr-4"     : {
            "Larawise\\Tests\\"             : "./tests"
        }
    },

    "extra"                 : {
        "laravel"   : {
            "providers"     : ["Larawise\\Iconify\\IconifyProvider"],
            "aliases"       : { "Iconify": "Larawise\\Iconify\\Facades\\Iconify" }
        }
    },

    "config"                : {
        "optimize-autoloader"   : true,
        "preferred-install"     : "dist",
        "sort-packages"         : true,
        "allow-plugins"         : {}
    },

    "minimum-stability"     : "dev",
    "prefer-stable"         : true
}
